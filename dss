#include <stdio.h>
#include <stdlib.h> 

int a[10],top=-1,max,iteam;

void push();
void pop();
void peek();

void push(){
if(top==max-1){
printf("the stack have over flowed\n");
}else{
top=top+1;
printf("enter the value : ");
scanf("%d\n",&iteam);
a[top]=iteam;
}
}

void pop(){
if(top==-1){
printf("empty\n");
}else{
iteam=a[top];
printf("%d\n",iteam);
top=top-1;
}
}

void peek(){
for(int i=top;i>=0;i--){
printf("%d\n",a[top]);
}
}




int main(){
int ch;


printf("enter the size stack : ");
scanf("%d\n",&max);



printf("1.To push in stack\n2.To pop in stack\n3.To peek in stack\n4.exit\n");
scanf("enter your choice :%d\n",&ch);

switch(ch){

case 1:
push();
break;
case 2:
pop();
break;
case 3:
peek();
break;




}

}
